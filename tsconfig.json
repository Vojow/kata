{
  "compilerOptions": {
    "allowJs": true,
    "declaration": true,
    "outDir": "dist",
    "target": "ES2019",
    "module": "CommonJS",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noEmitOnError": true,
    "noUnusedLocals": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "noEmit": false,
    "pretty": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": false,
    "lib": ["dom", "es5", "es6", "es2015", "es2015.promise", "ES2020", "ESNext"],
    "typeRoots": ["./node_modules/@types"],
    "types": ["node", "jest"],
    "baseUrl": ".",
    "paths": {},
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"],
  "compileOnSave": false
}
